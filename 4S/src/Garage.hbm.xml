<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="CarSaleManagerSystem.Bean.Garage" table="garage">
        <id name="brand" type="java.lang.String" length="64" column="brand">
        </id>
        <set name="carSet" table="vehicle_garage" inverse="true">
            <key column="brand" not-null="true"/>
            <one-to-many class="CarSaleManagerSystem.Bean.Car"/>
        </set>
    </class>
</hibernate-mapping>
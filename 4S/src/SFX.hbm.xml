<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="CarSaleManagerSystem.Bean.CarSFX" table="sfx">
        <id name="sfx" column="sfx" type="java.lang.String" length="64">
        </id>
        <set name="carTypeSet" table="vehicle_type" inverse="true">
            <key column="sfx" not-null="true"/>
            <one-to-many class="CarSaleManagerSystem.Bean.CarType"/>
        </set>
    </class>
</hibernate-mapping>